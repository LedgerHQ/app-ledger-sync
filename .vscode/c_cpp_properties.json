{
    "env": {
        "BOLOS_SDK": "/opt/nanos-secure-sdk"
    },
    "configurations": [
        {
            "name": "Nano S",
            "includePath": [
                "/usr/include",
                "${workspaceFolder}/src",
                "${workspaceFolder}/build/*/gen_src",
                "${env:BOLOS_SDK}/**"
            ],
            "defines": [
                "TARGET_NANOS",
                "OS_IO_SEPROXYHAL",
                "HAVE_BAGL",
                "HAVE_NBGL",
                "HAVE_SPRINTF",
                "HAVE_IO_USB",
                "HAVE_L4_USBLIB",
                "HAVE_ECC",
                "IO_USB_MAX_ENDPOINTS=6",
                "IO_HID_EP_LENGTH=64",
                "HAVE_USB_APDU",
                "USB_SEGMENT_SIZE=64",
                "UNUSED(x)=(void)x",
                "IO_SEPROXYHAL_BUFFER_SIZE_B=128",
                "HAVE_UX_FLOW",
                "DEBUG=1",
                "HAVE_PRINTF",
                "PRINTF=screen_printf"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}
